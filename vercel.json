{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://srv1307385.hstgr.cloud/api/:path*"
        },
        {
            "source": "/assets/:path*",
            "destination": "/assets/:path*"
        },
        {
            "source": "/:path+",
            "destination": "https://prime-ai.fr/:path+"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Clear-Site-Data",
                    "value": "\"cache\", \"executionContexts\""
                }
            ]
        }
    ]
}